for i, v in pairs(game:GetService("CoreGui"):GetChildren()) do
	if v.Name == "ui" then
		v:Destroy()
	end
end
local UI = loadstring(game:HttpGetAsync("https://pastebin.com/raw/0589YkmB"))()

local window = UI:Window("Premium")
local nif = UI:Notification("Welcome to", game:GetService("Players").LocalPlayer.Name, "Okay!")
local farm = window:Tab("AutoFarm")
local esp = window:Tab("Esp")
local player = window:Tab("LocalPlayer")

_G.PizzaFast = false
farm:Toggle("PizzaFast",false,function(value)
    _G.PizzaFast = value
end)

_G.Esp = true
esp:Toggle("Esp",true,function(value)
	_G.Esp = value
end)

_G.speedOn = true
player:Toggle("Speed On/Off",true,function(value)
	_G.speedOn = value
end)

_G.speed = 24
player:Slider("Speed",0,200,24,function(value)
	_G.speed = value
end)

local infJump
infJumpDebounce = false
player:Toggle("Inf Jump",false,function(value)
	if value == true then
		infJump = game:GetService("UserInputService").JumpRequest:Connect(function()
	    	if not infJumpDebounce then
	        infJumpDebounce = true
	        game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Humanoid"):ChangeState(Enum.HumanoidStateType.Jumping)
	        wait()
	        infJumpDebounce = false
	    	end
		end)
	else
		infJump:Disconnect()
	end
end)

_G.Noclip = false
player:Toggle("Noclip", false, function(value)
	_G.Noclip = value
end)

game:GetService("RunService").RenderStepped:Connect(function()
		if _G.Noclip then
          for i, v in game:GetService("Players").LocalPlayer.Character:GetChildren() do
              if v:IsA("BasePart") then
                  v.CanCollide = false
              end
          end
      end
	  if _G.speedOn then
		  game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = _G.speed
	  else
		 game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16		  
	  end
end)

local player = game.Players.LocalPlayer

local function pizzaUp()
    if not _G.PizzaFast then
        return
    end
    
    local character = player.Character
    if not character then return end
    
    local tool = character:FindFirstChildOfClass("Tool")
    if tool and tool:FindFirstChild("House") then
        return
    elseif tool and not tool:FindFirstChild("House") then
        tool.Parent = player.Backpack
    end
    
    if not character:FindFirstChildOfClass("Tool") then
        for _, v in player.Backpack:GetChildren() do
            if v:FindFirstChild("House") then
                v.Parent = character
                break  -- Equip only the first one
            end
        end
    end
end

local function goHome()
    local pizzaCount = 0
    repeat
        wait()
        if not _G.PizzaFast then return end
        
        pizzaCount = 0
        for _ in player.Backpack:GetChildren() do
            if v and v:FindFirstChild("House") then
                pizzaCount = pizzaCount + 1
            end
        end
        
        pizzaUp()
        
        local character = player.Character
        if not character or not character:FindFirstChildOfClass("Tool") then
            return
        end
        
        local tool = character:FindFirstChildOfClass("Tool")
        for _, house in workspace.Houses:GetChildren() do
            if house.Address.Value == tool.Name then
                for _, f in house:GetDescendants() do
                    if f.Name == "DoorTouch" then
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = f.CFrame + f.CFrame.RightVector * -2
						cf = CFrame.lookAt(game.Players.LocalPlayer.Character.HumanoidRootPart.Position, f.Position)
						game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = cf
                        break
                    end
                end
                break
            end
        end
        
        repeat
            wait()
            if not _G.PizzaFast then
                return
            end
        until not player.Character:FindFirstChildOfClass("Tool")
        
    until pizzaCount == 0
end

local function Give()
    if not _G.PizzaFast then return end
    
    for _, v in workspace:GetChildren() do
        if v:IsA("Tool") then
            player.Character.HumanoidRootPart.CFrame = v.Handle.CFrame
            wait(0.1)  -- Small delay to pick up
        end
    end
	wait(2)
    goHome()
end

spawn(function()
    while wait(1) do
        if _G.PizzaFast then
            Give()
        end
    end
end)
